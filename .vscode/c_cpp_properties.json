{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/MSVC/*/include/**",
                "C:/Program Files (x86)/Windows Kits/10/Include/*/**"
            ],
            "compilerPath": "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/MSVC/*/bin/Hostx64/x64/cl.exe",
            "intelliSenseMode": "windows-msvc-x64",
            "cStandard": "c17",
            "cppStandard": "c++23",
            "compileCommands": [
                "${workspaceFolder}/out/build/win64/debug/compile_commands.json"
            ]
        },
        {
            "name": "Linux-GCC",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include"
            ],
            "compilerPath": "/usr/bin/g++",
            "intelliSenseMode": "linux-gcc-x64",
            "cStandard": "c17",
            "cppStandard": "c++23",
            "compileCommands": [
                "${workspaceFolder}/out/build/linux/debug/compile_commands.json"
            ]
        },
        {
            "name": "GCC-MINGW64",
            "includePath": [
                "${workspaceFolder}/**",
                "d:/msys64/mingw64/include/**",
                "d:/msys64/mingw64/lib/gcc/x86_64-w64-mingw32/*/include/**"
            ],
            "compilerPath": "d:/msys64/mingw64/bin/g++.exe",
            "intelliSenseMode": "windows-gcc-x64",
            "cStandard": "c17",
            "cppStandard": "c++23",
            "compileCommands": [
                "${workspaceFolder}/out/build/gcc-mingw64/debug/compile_commands.json"
            ]
        },
        {
            "name": "Linux-Clang",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include",
                "/usr/lib/llvm-*/include"
            ],
            "compilerPath": "/usr/bin/clang++",
            "intelliSenseMode": "linux-clang-x64",
            "cStandard": "c17",
            "cppStandard": "c++23",
            "compileCommands": [
                "${workspaceFolder}/out/build/linux-clang/debug/compile_commands.json"
            ]
        },
        {
            "name": "Win-Clang",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "compilerPath": "clang++",
            "intelliSenseMode": "windows-clang-x64",
            "cStandard": "c17",
            "cppStandard": "c++23",
            "compileCommands": [
                "${workspaceFolder}/out/build/win-clang/debug/compile_commands.json"
            ]
        }
    ],
    "version": 4
}